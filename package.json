{
  "name": "retroweird-blog-theme",
  "version": "1.0.0",
  "description": "A maximalist Web 1.0 revival blog theme for Eleventy - inspired by GeoCities, MySpace, and personal homepages of the 1990s-2000s",
  "main": ".eleventy.js",
  "scripts": {
    "start": "eleventy --serve",
    "build": "eleventy",
    "debug": "DEBUG=Eleventy* eleventy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brennanbrown/retroweird.git"
  },
  "keywords": [
    "eleventy",
    "blog",
    "theme",
    "retro",
    "web1.0",
    "geocities",
    "myspace",
    "indieweb",
    "nostalgic"
  ],
  "author": "Brennan Kenneth Brown <mail@brennanbrown.ca> (https://brennanbrown.ca)",
  "license": "MIT",
  "dependencies": {
    "@11ty/eleventy": "^2.0.1",
    "@11ty/eleventy-plugin-rss": "^1.2.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.0",
    "markdown-it": "^13.0.2",
    "markdown-it-attrs": "^4.1.6"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=18.0.0"
  }
}
